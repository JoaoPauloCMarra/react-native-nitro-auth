{
  "name": "react-native-nitro-auth-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "node scripts/setup.js",
    "publish-package": "node scripts/publish.js",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "codegen": "turbo run codegen",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "test:cpp": "turbo run test:cpp --filter=react-native-nitro-auth",
    "start": "turbo run start --filter=example --",
    "example:ios": "turbo run ios --filter=example",
    "example:android": "turbo run android --filter=example",
    "example:prebuild": "turbo run prebuild --filter=example",
    "example:prebuild:clean": "turbo run prebuild:clean --filter=example"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.17",
    "jest": "^29.7.0",
    "nitrogen": "^0.33.9",
    "react-native-builder-bob": "^0.40.18",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-nitro-modules": "^0.33.9",
    "react-native-web": "^0.21.2"
  },
  "packageManager": "bun@1.3.9"
}
